#include <bits/stdc++.h> 
#include <iostream>
using namespace std;

int main() {
    //Write your code here
    int N;
    cin>>N;
    int a[N];
    for(int i=0;i<N;i++)
    {
        cin>>a[i];
    }
    bool flag=0;
    int X;
    cin>>X;
    for(int i=N-1;i>=0;i--)
    {
        if(a[i]==X)
        {
            flag=1;
            cout<<i;
            break;
        }
    }
    if(flag==0)
    cout<<-1;
    return 0;
}
