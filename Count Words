#include <bits/stdc++.h> 
#include <iostream>
#include <cstring>
using namespace std;

int countWords(string input) {
    // Write your code here
    int c=0;
    for(int i=0;i<input.size();i++)
    {
        if(input[i]==' ')
        c++;
    }
    return c+1;
}

int main() {
    string input;
    getline(cin, input);
    int output = countWords(input);
    cout << output << endl;
    return 0;
}
