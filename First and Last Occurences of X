
vector <int> find(int arr[], int n , int x )
 {
     int a,b;
    for(int i=0;i<n;i++)
    {
        if(arr[i]==x)
        {
            a=i;
            break;
        }
        else
        {
            a=-1;
        }
    }
    
    for(int i=n-1;i>=0;i--)
    {
        if(arr[i]==x)
        {
            b=i;
            break;
        }
        else
        {
            b=-1;
        }
    }
    
    return {a,b};
 }
 
 
